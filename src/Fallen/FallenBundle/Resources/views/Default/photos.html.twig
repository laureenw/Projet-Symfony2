{% extends "::layout.html.twig" %}

{% block body %}
<div class="container">
    <hr>
    <div class="row">


    <div class="col-xs-9">
        <!-- Tab panes -->
        <div class="tab-content">
            {% for image in files %}
                <a class="fancybox" rel="gallery1" href="{{ asset(image) }}" title="">
                    <img style="width:20%" src="{{ asset(image) }}" alt="" />
                </a>  
            {% endfor %}
       </div>
    </div>

    <div class="clearfix"></div>

    </div>
</div>
<script type="text/javascript">
$(document).ready(function() {
	$(".fancybox").fancybox({
		openEffect	: 'none',
		closeEffect	: 'none'
	});
});
</script>
{% endblock %}

